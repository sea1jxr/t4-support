{
    "name": "t4-support",
    "displayName": "T4 Support",
    "description": "Provides syntax highlighting for T4 Text Templates.",
    "version": "0.1.0",
    "publisher": "zbecknell",
    "engines": {
        "vscode": "^1.20.0"
    },
    "categories": [
        "Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "tt",
            "aliases": ["T4 Text Template", "tt"],
            "extensions": [".tt"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "tt",
            "scopeName": "source.tt",
            "path": "./syntaxes/tt.tmLanguage.json"
        }]
    }
}